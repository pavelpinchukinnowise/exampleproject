{
  "Logging": {
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureMaps": {
    "AuthOptions": {
      "SubscriptionKey": ""
    }
  },
  "CosmosDb": {
    "ConnectionString": ""
  },
  "Auth": {
    "ApplicationSecurity": {
      "AuthApiUrl": "#{Auth.ApplicationRegistration.AuthApiUrl}#",
      "ApplicationId": "#{Auth.ApplicationRegistration.ApplicationId}#",
      "ApplicationSecret": "#{Auth.ApplicationRegistration.ApplicationSecret}#"
    },
    "ApplicationRegistration": {
      "Instance": "#{Auth.ApplicationRegistration.Instance}#",
      "Domain": "#{Auth.ApplicationRegistration.Domain}#",
      "ClientId": "#{Auth.ApplicationRegistration.ClientId}#",
      "SwaggerClientId": "#{Auth.ApplicationRegistration.SwaggerClientId}#",
      "SignUpSignInPolicyId": "#{Auth.ApplicationRegistration.SignUpSignInPolicyId}#",
      "Scopes": {
        "ForwardingManagement": "#{Auth.ApplicationRegistration.ForwardingManagement}#"
      }
    }
  }
}
